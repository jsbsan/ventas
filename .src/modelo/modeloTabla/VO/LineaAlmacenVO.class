    dato = Me 'se copia a si mismo
  Endif
  datoVOtmp.id = dato.id
  datoVOtmp.idProducto = dato.idProducto
  datoVOtmp.cantidad = dato.cantidad
  datoVOtmp.tipo = dato.tipo
