PUBLIC FUNCTION Copia(Optional dato as ReciboVO) AS ReciboVO
Dim datoVOtmp as new ReciboVO
Dim datoVOtmpOriginal as ReciboVO
If isnull(dato) then
dato=me 'se copia a si mismo
endif
datoVOtmp.id=dato.id
datoVOtmp.idempleado=dato.idempleado
datoVOtmp.idcliente=dato.idcliente
datoVOtmp.fecha=dato.fecha
datoVOtmp.idlinearecibo=dato.idlinearecibo
datoVOtmp.importe=dato.importe
return datoVOtmp
end

PUBLIC FUNCTION ConvertirVOaColeccion(dato as ReciboVO) AS collection
Dim colecciontmp as new collection
colecciontmp.add(dato.id,"id")
colecciontmp.add(dato.idempleado,"idempleado")
colecciontmp.add(dato.idcliente,"idcliente")
colecciontmp.add(dato.fecha,"fecha")
colecciontmp.add(dato.idlinearecibo,"idlinearecibo")
colecciontmp.add(dato.importe,"importe")
return colecciontmp
end
PUBLIC FUNCTION ConvertirColeccionaVO(dato as collection) AS ReciboVO
Dim datoVOtmp as new ReciboVO
try datoVOtmp.id=dato["id"]
try datoVOtmp.idempleado=dato["idempleado"]
try datoVOtmp.idcliente=dato["idcliente"]
try datoVOtmp.fecha=dato["fecha"]
try datoVOtmp.idlinearecibo=dato["idlinearecibo"]
try datoVOtmp.importe=dato["importe"]
return datoVOtmp
end
