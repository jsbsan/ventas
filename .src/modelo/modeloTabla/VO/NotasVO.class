PUBLIC FUNCTION Copia(Optional dato as NotasVO) AS NotasVO
Dim datoVOtmp as new NotasVO
Dim datoVOtmpOriginal as NotasVO
If isnull(dato) then
dato=me 'se copia a si mismo
endif
datoVOtmp.id=dato.id
datoVOtmp.idcliente=dato.idcliente
datoVOtmp.fecha=dato.fecha
datoVOtmp.Comentario=dato.Comentario
return datoVOtmp
end

PUBLIC FUNCTION ConvertirVOaColeccion(dato as NotasVO) AS collection
Dim colecciontmp as new collection
colecciontmp.add(dato.id,"id")
colecciontmp.add(dato.idcliente,"idcliente")
colecciontmp.add(dato.fecha,"fecha")
colecciontmp.add(dato.Comentario,"Comentario")
return colecciontmp
end
PUBLIC FUNCTION ConvertirColeccionaVO(dato as collection) AS NotasVO
Dim datoVOtmp as new NotasVO
try datoVOtmp.id=dato["id"]
try datoVOtmp.idcliente=dato["idcliente"]
try datoVOtmp.fecha=dato["fecha"]
try datoVOtmp.Comentario=dato["Comentario"]
return datoVOtmp
end
