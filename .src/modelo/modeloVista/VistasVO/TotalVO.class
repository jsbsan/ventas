PUBLIC FUNCTION ConvertirVOaColeccion(dato as TotalVO) AS collection
Dim colecciontmp as new collection
colecciontmp.add(dato.total3_fecha,"total3_fecha")
colecciontmp.add(dato.total3_codigo,"total3_codigo")
colecciontmp.add(dato.total3_nombre,"total3_nombre")
colecciontmp.add(dato.total3_cantidad,"total3_cantidad")
colecciontmp.add(dato.total3_precio,"total3_precio")
colecciontmp.add(dato.total3_descuento,"total3_descuento")
colecciontmp.add(dato.total3_iva,"total3_iva")
colecciontmp.add(dato.cliente,"cliente")
colecciontmp.add(dato.empleado,"empleado")
return colecciontmp
end
PUBLIC FUNCTION ConvertirColeccionaVO(dato as collection) AS TotalVO
Dim datoVOtmp as new TotalVO
try datoVOtmp.total3_fecha=dato["total3_fecha"]
try datoVOtmp.total3_codigo=dato["total3_codigo"]
try datoVOtmp.total3_nombre=dato["total3_nombre"]
try datoVOtmp.total3_cantidad=dato["total3_cantidad"]
try datoVOtmp.total3_precio=dato["total3_precio"]
try datoVOtmp.total3_descuento=dato["total3_descuento"]
try datoVOtmp.total3_iva=dato["total3_iva"]
try datoVOtmp.cliente=dato["cliente"]
try datoVOtmp.empleado=dato["empleado"]
return datoVOtmp
end
