PUBLIC FUNCTION ConvertirVOaColeccion(dato as SumaEntradaVO) AS collection
Dim colecciontmp as new collection
colecciontmp.add(dato.producto_codigo,"producto_codigo")
colecciontmp.add(dato.producto_nombre,"producto_nombre")
colecciontmp.add(dato.precio,"precio")
colecciontmp.add(dato.lineaAlmacen_idproducto,"lineaAlmacen_idproducto")
colecciontmp.add(dato.suma_ENTRADA,"suma_ENTRADA")
return colecciontmp
end
PUBLIC FUNCTION ConvertirColeccionaVO(dato as collection) AS SumaEntradaVO
Dim datoVOtmp as new SumaEntradaVO
try datoVOtmp.producto_codigo=dato["producto_codigo"]
try datoVOtmp.producto_nombre=dato["producto_nombre"]
try datoVOtmp.precio=dato["precio"]
try datoVOtmp.lineaAlmacen_idproducto=dato["lineaAlmacen_idproducto"]
try datoVOtmp.suma_ENTRADA=dato["suma_ENTRADA"]
return datoVOtmp
end
