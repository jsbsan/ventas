PUBLIC FUNCTION ConvertirVOaColeccion(dato as INVENTARIOVALORADOVO) AS collection
Dim colecciontmp as new collection
colecciontmp.add(dato.sumaEntrada_codigo,"sumaEntrada_codigo")
colecciontmp.add(dato.sumaEntrada_nombre,"sumaEntrada_nombre")
colecciontmp.add(dato.sumaEntrada_idproducto,"sumaEntrada_idproducto")
colecciontmp.add(dato.sumaEntrada_precio,"sumaEntrada_precio")
colecciontmp.add(dato.ENTRADA,"ENTRADA")
colecciontmp.add(dato.COSTE_ENTRADA,"COSTE_ENTRADA")
colecciontmp.add(dato.VENTA,"VENTA")
colecciontmp.add(dato.COSTE_VENTA,"COSTE_VENTA")
colecciontmp.add(dato.ROTURA,"ROTURA")
colecciontmp.add(dato.COSTE_ROTURA,"COSTE_ROTURA")
colecciontmp.add(dato.ROBO,"ROBO")
colecciontmp.add(dato.COSTE_ROBO,"COSTE_ROBO")
colecciontmp.add(dato.EXISTENCIA_ALMACEN,"EXISTENCIA_ALMACEN")
colecciontmp.add(dato.COSTE_ALMACEN,"COSTE_ALMACEN")
return colecciontmp
end
PUBLIC FUNCTION ConvertirColeccionaVO(dato as collection) AS INVENTARIOVALORADOVO
Dim datoVOtmp as new INVENTARIOVALORADOVO
try datoVOtmp.sumaEntrada_codigo=dato["sumaEntrada_codigo"]
try datoVOtmp.sumaEntrada_nombre=dato["sumaEntrada_nombre"]
try datoVOtmp.sumaEntrada_idproducto=dato["sumaEntrada_idproducto"]
try datoVOtmp.sumaEntrada_precio=dato["sumaEntrada_precio"]
try datoVOtmp.ENTRADA=dato["ENTRADA"]
try datoVOtmp.COSTE_ENTRADA=dato["COSTE_ENTRADA"]
try datoVOtmp.VENTA=dato["VENTA"]
try datoVOtmp.COSTE_VENTA=dato["COSTE_VENTA"]
try datoVOtmp.ROTURA=dato["ROTURA"]
try datoVOtmp.COSTE_ROTURA=dato["COSTE_ROTURA"]
try datoVOtmp.ROBO=dato["ROBO"]
try datoVOtmp.COSTE_ROBO=dato["COSTE_ROBO"]
try datoVOtmp.EXISTENCIA_ALMACEN=dato["EXISTENCIA_ALMACEN"]
