PUBLIC FUNCTION ConvertirVOaColeccion(dato as VistaAlbaranesAlmacenVO) AS collection
Dim colecciontmp as new collection
colecciontmp.add(dato.lineaAlmacen_id,"lineaAlmacen_id")
colecciontmp.add(dato.fecha,"fecha")
colecciontmp.add(dato.idProducto,"idProducto")
colecciontmp.add(dato.codigo,"codigo")
colecciontmp.add(dato.nombre,"nombre")
colecciontmp.add(dato.cantidad,"cantidad")
colecciontmp.add(dato.tipo,"tipo")
return colecciontmp
end
PUBLIC FUNCTION ConvertirColeccionaVO(dato as collection) AS VistaAlbaranesAlmacenVO
Dim datoVOtmp as new VistaAlbaranesAlmacenVO
try datoVOtmp.lineaAlmacen_id=dato["lineaAlmacen_id"]
try datoVOtmp.fecha=dato["fecha"]
try datoVOtmp.idProducto=dato["idProducto"]
try datoVOtmp.codigo=dato["codigo"]
try datoVOtmp.nombre=dato["nombre"]
try datoVOtmp.cantidad=dato["cantidad"]
try datoVOtmp.tipo=dato["tipo"]
return datoVOtmp
end
